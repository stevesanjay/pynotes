    "def update_df(file_name):\n",
    "    #read dataframe\n",
    "    df = pd.read_excel(file_name)\n",
    "\n",
    "    df.loc[df['name']=='jerin','age'] = 21\n",
    "\n",
    "    df.to_excel(file_name,index=False)\n",
    "\n",
    "    return df"
