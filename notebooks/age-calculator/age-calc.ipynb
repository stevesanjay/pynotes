    "from datetime import datetime"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "8bea937c-fc90-43e2-bcb6-2daf9fa5f1be",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_age(d):\n",
    "    d1 = datetime.now()\n",
    "    months = (d1.year - d.year) * 12 + d1.month - d.month\n",
    "\n",
    "    year = int(months / 12)\n",
    "    return year"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "f3986faa-f425-4eab-ad78-bcb1ecb2f559",
   "metadata": {},
   "outputs": [],
   "source": [
    "age = get_age(datetime(2002, 12, 14))"
   ]
  },
